@model IEnumerable<WebNinnoFeliz.Models.AbonadorCargoMensuale>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.IdAbonadorNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdCargoNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdAbonadorNavigation.IdAbonador)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdCargoNavigation.IdCargo)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdAbonadorCargoMensual">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdAbonadorCargoMensual">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdAbonadorCargoMensual">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
