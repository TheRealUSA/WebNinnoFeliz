            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdEncargadoNavigation.Apell2Encargado) 
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdEncargadoNavigation.IdParentezco) 
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdNinnoNavigation.NombreNinno)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdNinnoNavigation.Apell1Ninno)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdNinnoNavigation.Apell2Ninno) 
            </td>
        </tr>
        }
    </tbody>
</table>
